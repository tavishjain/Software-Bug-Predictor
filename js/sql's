*for id @DynamoDBAttribute is not mentioned still treating as primary key*

CREATE TABLE
`icat_excel_configuration_prod`(
	`id` varchar(100) NOT NULL,
	`primaryKey` varchar(100) NOT NULL,
  `sheetName` varchar(100) DEFAULT 'HUL',
  `parentHeaders` boolean NOT NULL,
  `fieldDescription` longtext NOT NULL,
  `parentHeaderColor` longtext NOT NULL,
  `headers` longtext NOT NULL,
 	 PRIMARY KEY (`id`) 
)ENGINE = InnoDB;


----------------------------------------------------

*TINYTEXT - 64KB MEDIUMTEXT - 16MB LONGTEXT - 4GB*

CREATE TABLE
`icat_fetch_contract_status_log_prod`(
	`fetchApiCounter` long NOT NULL,
	`startTime` varchar(100) NOT NULL,
	`endTime` varchar(100) NOT NULL,
	`responseCode` TINYTEXT NOT NULL,
	`responseJson` MEDIUMTEXT NOT NULL,
	`error` MEDIUMTEXT NOT NULL,
	PRIMARY KEY(fetchApiCounter)
)ENGINE = InnoDB;