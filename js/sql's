CREATE TABLE `icat_upload_operations_prod`(
	`id` varchar(100) NOT NULL,
	`status` int NOT NULL,
	`message` varchar(500) NOT NULL,
	`sheetName` varchar(100) NOT NULL,
	`currenttime` long NOT NULL,
	`totalRowsInExcel` int NOT NULL,
	`totalAddedRows` int NOT NULL,
	PRIMARY KEY (`id`)
)ENGINE = InnoDB;


