CREATE TABLE `icat_master_data_upload_report_prod`(
	 `jobID` varchar(100) NOT NULL,
	 `date` varchar(100) NOT NULL,
	 `userStatus` varchar(100) NOT NULL,
	 `status` boolean NOT NULL,
	 `startTime` long NOT NULL,
	 `endTime` long NOT NULL,
	 `totalTimeTaken` long NOT NULL,
	 `indexReportList` longtext NOT NULL,
	 `switchedIndices` longtext NOT NULL,
	 PRIMARY KEY (`jobID`)
)ENGINE=InnoDB;

