      "accent-1":      "#0085c7",
      "toggled":       "#252525"
    },
    "group-3": {
      "background":    "#323232",
      "text":          {"active": "#dedede", "inactive": "#dedede", "disabled": "#b4b1b1" },
      "link":          "#0081e1",
      "link-visited":  "#8d1aff",
      "accent-1":      "#0085c7"
    }
  },
