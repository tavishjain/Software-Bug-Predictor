{
  "display-name":      "Light Theme",
  "description":       "The official light theme supported by Scientific Toolworks, Inc.",
  "defaults": {
    "explanation":     "Colors are grouped by background. Variants such as active, inactive, and disabled are optional.",
    "group-1": {
      "background":    "#ffffff",
      "accent-1":      "#3466ba",
      "accent-1-text": "#f8fbfe",
      "border":        "#dfdfdf",
      "error":         "#93263c",
      "link":          "#2211c7",
      "link-visited":  "#c383cf",
      "placeholder":   "#989898",
      "text":          "#080808",
      "warning":       "#f15a22"
    },
    "group-2": {
      "background":    "#ececec",
      "border":        "#bebebe",
      "accent-1":      "#3466ba",
      "accent-1-text": "#f8fbfe",
      "tab":           "#ffffff",
      "tab-text":      "#080808",
      "text":          "#545555"
    },
    "group-3": {
      "background":    "#d4d4d4",
      "text":          {"active": "#545555", "disabled": "#949494", "inactive": "#949494"},
      "accent-1":      "#3466ba",
      "toggled":       "#bdbdbd"
    },
    "group-4": {
      "background":    "#bfbfbf",
      "border":        "#7f7f7f",
      "text":          {"active": "#545555", "disabled": "#7a7b7b"},
      "accent-1":      "#3466ba"
    }
  },
